<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Canvas fx:id="canvasTrack" height="200.0" width="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="forme1" mnemonicParsing="false" text="Button" onAction="#handleForme1Action"/>
            <Button fx:id="forme2" mnemonicParsing="false" text="Button" onAction="#handleForme2Action"/>
            <Button fx:id="forme3" mnemonicParsing="false" text="Button" onAction="#handleForme3Action"/>
         </children>
      </HBox>
   </top>
</BorderPane>
